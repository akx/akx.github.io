{
  "extends": "airbnb-base",
  "rules": {
    "func-names": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "no-var": 0,
    "object-curly-spacing": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-rest-params": 0,
    "prefer-template": 0,
    "vars-on-top": 0,
  }
}
